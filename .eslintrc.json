{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "standard",
        "prettier"
    ],
    "plugins": [
        "prettier"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "camelcase": "off",
        "no-undef": "off",
        "no-mixed-operators": "off",
        "no-void": "off",
        "no-unused-vars": "off",
        "no-undef-init": "off",
        "no-unsafe-finally": "off",
        "eqeqeq": "off",
        "no-new-func": "off"
    }
}